    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Python Debugger: StartAPI with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "start_api.py",
            "console": "integratedTerminal",
            "python": "${command:python.interpreterPath}",
            "args": "${command:pickArgs}"
